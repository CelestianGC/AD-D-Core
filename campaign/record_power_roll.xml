<root>
	<windowclass name="power_cast_editor">
		<frame>charsheethelper</frame>
		<placement>
			<size width="370" height="350" />
		</placement>
		<sizelimits>
			<minimum width="370" height="350" />
			<dynamic />
		</sizelimits>
		<nodelete />
		<playercontrol />
		<minimize>minimized_char</minimize>
		<tooltip control="name" />
		<sheetdata>
			<windowtitlebar_char name="title">
				<resource>power_title_cast</resource>
			</windowtitlebar_char>
			<anchor_title_charsheethelper name="contentanchor" />
			
			<genericcontrol name="contentframe">
				<anchored to="contentanchor">
					<top anchor="bottom" offset="0" />
					<left />
					<right />
					<bottom parent="" offset="-30" />
				</anchored>
			</genericcontrol>

			<subwindow_record name="main">
				<class>power_cast_editor_main</class>
				<activate />
				<fastinit />
			</subwindow_record>

			<scrollbar_record>
				<target>main</target>
			</scrollbar_record>

			<resize_charsheethelper />
			<close_charsheethelper />
		</sheetdata>
	</windowclass>
	<windowclass name="power_cast_editor_main">
		<script>
			function onInit()
				updateDisplay();
			end
			
			function updateDisplay()
				local node = getDatabaseNode();
				local bAtkAbilityMode = (DB.getValue(node, "atkbase", "") == "ability");
				local bSaveAbilityMode = (DB.getValue(node, "savedcbase", "") == "ability");
				
				atkstat.setVisible(bAtkAbilityMode);
				label_atkstat.setVisible(bAtkAbilityMode);
				atkplus.setVisible(bAtkAbilityMode);
				atkmod.setAnchor("left", "atkplus", "right", "absolute", 10);

				savedcmod.setAnchor("left", "savetype", "right", "absolute", 10);
			end
		</script>
		<sheetdata>
			<anchor_column name="columnanchor" />
			
			<label_column name="name_label">
				<static textres="power_label_name" />
			</label_column>
			<stringcontrol name="name">
				<anchored>
					<top parent="columnanchor" anchor="bottom" relation="relative" offset="7" />
					<left offset="97" />
					<right offset="-5" />
				</anchored>
				<multilinespacing>20</multilinespacing>
				<font>sheettext</font>
				<readonly />
			</stringcontrol>
			
			<header_column name="header_attack">
				<static textres="power_header_cast_attack" />
				<center />
			</header_column>
			<button_stringcycler name="atktype">
				<anchored width="60" height="20">
					<top parent="columnanchor" anchor="bottom" relation="relative" offset="20" />
					<left offset="5" />
				</anchored>
				<parameters>
					<defaultlabelres>dash</defaultlabelres>
					<labelsres>melee|ranged</labelsres>
					<values>melee|ranged</values>
				</parameters>
			</button_stringcycler>
			<label_fieldtop>
				<anchored to="atktype" />
				<static textres="power_label_atktype" />
			</label_fieldtop>
			<button_stringcycler name="atkbase">
				<anchored to="atktype" position="righthigh" offset="10,0" width="80" height="20" />
				<parameters>
<!-- 					<defaultlabelres>power_label_atkbase_group</defaultlabelres>
					<labelsres>power_label_atkbase_ability|power_label_atkbase_fixed</labelsres>
					<values>ability|fixed</values> -->
                    <defaultlabelres>dash</defaultlabelres>
					<labelsres>power_label_atkbase_fixed|power_label_atkbase_ability</labelsres>
					<values>fixed|ability</values>
				</parameters>
				<script>
					function onValueChanged()
						window.updateDisplay();
					end
				</script>
			</button_stringcycler>
			<label_fieldtop>
				<anchored to="atkbase" />
				<static textres="power_label_atkbase" />
			</label_fieldtop>
			<cycler_power name="atkstat">
				<anchored to="atkbase" position="righthigh" offset="10,0" />
			</cycler_power>
			<label_fieldtop name="label_atkstat">
				<anchored to="atkstat" />
				<static textres="power_label_atkstat" />
			</label_fieldtop>
			<label name="atkplus">
				<anchored to="atkstat" position="righthigh" offset="5,0" width="10" />
				<static>+</static>
				<center />
			</label>

			<basicnumber name="atkmod">
				<anchored to="atkplus" position="righthigh" offset="5,0" width="25" height="20" />
				<hideonvalue value="0" />
			</basicnumber>
			<label_fieldtop>
				<anchored to="atkmod" />
				<static textres="power_label_atkmod" />
			</label_fieldtop>
			
			<header_column name="header_save">
				<static textres="power_header_cast_save" />
				<center />
			</header_column>
			<cycler_power_savetype name="savetype">
				<anchored>
					<top parent="columnanchor" anchor="bottom" relation="relative" offset="20" />
					<left offset="5" />
				</anchored>
			</cycler_power_savetype>
			<label_fieldtop>
				<anchored to="savetype" />
				<static textres="power_label_savetype" />
			</label_fieldtop>

            
			<basicnumber name="savedcmod">
				<anchored to="savetype" position="righthigh" offset="5,0" width="25" height="20" />
				<hideonvalue value="0" />
			</basicnumber>
			<label_fieldtop>
				<anchored to="savedcmod" />
				<static textres="power_label_savedcmod" />
			</label_fieldtop>

			<header_column name="header_onmiss">
				<static textres="power_header_cast_onmiss" />
				<center />
			</header_column>
			<cycler_power_onmissdamage name="onmissdamage">
				<anchored>
					<top parent="columnanchor" anchor="bottom" relation="relative" offset="20" />
					<left offset="5" />
				</anchored>
			</cycler_power_onmissdamage>
			<label_fieldtop>
				<anchored to="onmissdamage" />
				<static textres="power_label_onmissdamage" />
			</label_fieldtop>
		</sheetdata>
	</windowclass>


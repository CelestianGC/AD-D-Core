				<static>+</static>
			</label>
 
			<cycler_power name="stat">
				<anchored to="label_plus_custom" position="righthigh" offset="2,0" width="30" height="20" />
			</cycler_power>

			<label name="label_plus2">
				<anchored to="stat" position="righthigh" offset="5,0" width="10" />
				<static>+</static>
			</label>
			<basicnumber name="bonus">
				<anchored to="label_plus2" position="righthigh" offset="2,0" width="35" height="20" />
				<hideonvalue>0</hideonvalue>
			</basicnumber>
      
      
			<label name="label_x">
				<anchored to="label_dee" position="belowleft" width="10" offset="0,3" />
				<static>X</static>
			</label>
      
			<basicnumber name="customvalue">
				<anchored to="dice" position="belowleft" width="60" height="20" offset="0,3" />
        <hideonvalue>0</hideonvalue>
        <tooltip textres="power_label_customvalue_tooltip" />
			</basicnumber>
      
		</sheetdata>
	</windowclass>

	<windowclass name="power_effect_editor">
		<frame>charsheethelper</frame>
		<placement>
			<size width="350" height="350" />
		</placement>
		<sizelimits>
			<minimum width="350" height="350" />
			<dynamic />
		</sizelimits>
		<nodelete />
		<playercontrol />
		<minimize>minimized_char</minimize>
		<tooltip control="name" />
		<sheetdata>
			<windowtitlebar_char name="title">
				<resource>power_title_effect</resource>
			</windowtitlebar_char>
			<anchor_title_charsheethelper name="contentanchor" />
			
			<genericcontrol name="contentframe">
				<anchored to="contentanchor">
					<top anchor="bottom" offset="0" />
					<left />
					<right />
					<bottom parent="" offset="-30" />
				</anchored>
			</genericcontrol>

			<subwindow_record name="main">
				<class>power_effect_editor_main</class>
				<activate />
				<fastinit />
			</subwindow_record>

			<scrollbar_record>
				<target>main</target>
			</scrollbar_record>

			<resize_charsheethelper />
			<close_charsheethelper />
		</sheetdata>
	</windowclass>
	<windowclass name="power_effect_editor_main">
		<sheetdata>
			<anchor_column name="columnanchor" />
			
			<label_column name="name_label">
				<static textres="power_label_name" />
			</label_column>
			<stringcontrol name="name">
				<anchored>
					<top parent="columnanchor" anchor="bottom" relation="relative" offset="7" />
					<left offset="97" />
					<right offset="-5" />
				</anchored>
				<multilinespacing>20</multilinespacing>
				<font>sheettext</font>
				<readonly />
			</stringcontrol>

			<header_column name="header_label">
				<static textres="power_header_effect" />
				<center />
			</header_column>
			<string_column_full name="label" />
			
			<header_column name="header_duration">
				<static textres="power_header_effect_duration" />
				<center />
			</header_column>
      
			<label_simple name="label_castertype">
				<anchored width="25" height="20">
					<top parent="columnanchor" anchor="bottom" relation="relative" offset="2" />
					<left offset="5" />
				</anchored>
				<static textres="power_label_castertype" />
        <tooltip textres="power_label_castertype_duration_tooltip" />
			</label_simple>
			<label_simple name="label_casterlevel">
				<anchored to="label_castertype" position="righthigh" offset="10,3" width="25" />
				<static textres="power_label_casterlevelmax" />
        <tooltip textres="power_label_casterlevelmax_duration_tooltip" />
			</label_simple>
			<label_simple name="label_durvalue">
				<anchored to="label_casterlevel" position="righthigh" offset="25,0" width="25" />
				<static textres="power_label_duration_value" />
        <tooltip textres="power_label_duration_value_tooltip" />
			</label_simple>
			<label_simple name="label_durdice">
				<anchored to="label_durvalue" position="righthigh" offset="15,0" width="60" />
				<static textres="power_label_duration_dice" />
        <tooltip textres="power_label_duration_dice_tooltip" />
			</label_simple>
			<label_simple name="label_durmod">
				<anchored to="label_durdice" position="righthigh" offset="10,0" width="30" />
				<static textres="text_MOD" />
        <tooltip textres="power_label_duration_mod_tooltip" />
			</label_simple>
			<label_simple>
				<anchored to="label_durmod" position="righthigh" offset="15,0" width="30" />
				<static textres="power_label_duration_unit" />
        <tooltip textres="power_label_duration_unit_tooltip" />
			</label_simple>

      <!--  --celestian -->
      <cycler_casterlevel_type name="castertype">
				<anchored width="25" height="20">
					<top parent="columnanchor" anchor="bottom" relation="relative" offset="1" />
					<left offset="5" />
				</anchored>
        <!-- <anchored position="insidetopleft" offset="5,1" width="25" height="20" /> -->
      </cycler_casterlevel_type>
      
			<label name="label_wrap1">
				<anchored to="castertype" position="righthigh" offset="4,0" width="10" />
				<static>(</static>
			</label>
			<basicnumber name="castermax">
				<anchored to="label_wrap1" position="righthigh" offset="-1,1" width="20" height="20" />
				<hideonvalue>0</hideonvalue>
        <default>0</default>
			</basicnumber>
			<label name="label_wrap2">
				<anchored to="castermax" position="righthigh" offset="3,0" width="10" />
				<static>)</static>
			</label>
      
			<label name="label_ex">
				<anchored to="label_wrap2" position="righthigh" offset="-2,0" width="10" />
				<static>X</static>
			</label>

			<basicnumber name="durvalue">
				<anchored to="label_ex" position="righthigh" offset="4,0" width="30" height="20" />
				<hideonvalue value="0" />
        <tooltip textres="power_label_duration_value_tooltip" />
			</basicnumber>

			<label name="label_plus2">
				<anchored to="durvalue" position="righthigh" offset="4,0" width="10" />
				<static>+</static>
			</label>

			<basicdice name="durdice">
        <anchored to="label_plus2" position="righthigh" offset="3,0" width="50" height="20" />
				<hideonvalue value="0" />
        <tooltip textres="power_label_duration_dice_tooltip" />
      </basicdice>
      
			<label name="label_plus">
				<anchored to="durdice" position="righthigh" offset="3,0" width="10" />
				<static>+</static>
			</label>
			<basicnumber name="durmod">
				<anchored to="label_plus" position="righthigh" offset="3,0" width="30" height="20" />
				<hideonvalue value="0" />
        <tooltip textres="power_label_duration_mod_tooltip" />
			</basicnumber>
			<button_stringcycler name="durunit">
				<anchored to="durmod" position="righthigh" offset="10,0" width="40" height="20" />
				<parameters>
					<defaultlabelres>effect_label_durunitrnd</defaultlabelres>
					<labelsres>effect_label_durunitmin|effect_label_durunithr|effect_label_durunitday</labelsres>
					<values>minute|hour|day</values>
				</parameters>
        <tooltip textres="power_label_duration_unit_tooltip" />
			</button_stringcycler>
			
			<header_column name="header_other">
				<static textres="power_header_effect_other" />
				<center />
			</header_column>
			
			<label_column name="targeting_label">
				<static textres="power_label_targeting" />
			</label_column>
			<button_stringcycler name="targeting">
				<anchored width="60" height="20">
					<top parent="columnanchor" anchor="bottom" relation="relative" offset="7" />
					<left offset="97" />
				</anchored>
				<parameters>
					<defaultlabelres>power_label_targetnormal</defaultlabelres>
					<labelsres>power_label_targetself</labelsres>
					<values>self</values>
				</parameters>
			</button_stringcycler>
			
			<label_column name="apply_label">
				<static textres="power_label_effect_apply" />
			</label_column>
			<button_stringcycler name="apply">
				<anchored width="120" height="20">
					<top parent="columnanchor" anchor="bottom" relation="relative" offset="7" />
					<left offset="97" />
				</anchored>
				<parameters>
					<defaultlabelres>power_label_effect_applyall</defaultlabelres>
					<labelsres>power_label_effect_applyaction|power_label_effect_applyroll|power_label_effect_applysingle</labelsres>
					<values>action|roll|single</values>
				</parameters>
			</button_stringcycler>
		</sheetdata>
	</windowclass>
</root>

			<label name="label_wrap2">
				<anchored to="castermax" position="righthigh" offset="3,0" width="10" />
				<static>)</static>
			</label>
      
			<label name="label_dee">
				<anchored to="label_wrap2" position="righthigh" offset="-2,0" width="10" />
				<static>D</static>
			</label>
      
			<basicdice name="dice">
				<anchored to="label_dee" position="righthigh" offset="4,1" width="60" height="20" />
				<!-- <anchored position="insidetopleft" offset="45,2" width="60" height="20" /> -->
        <tooltip textres="weapon_label_dmgdice_tooltip" />
			</basicdice>
      
			<label name="label_plus">
				<anchored to="dice" position="righthigh" offset="5,1" width="10" />
				<static>+</static>
			</label>

			<basicdice name="dicecustom">
				<anchored to="label_plus" position="righthigh" offset="4,1" width="60" height="20" />
				<!-- <anchored position="insidetopleft" offset="45,2" width="60" height="20" /> -->
        <tooltip textres="weapon_label_customdmgdice_tooltip" />
			</basicdice>
      
			<label name="label_plus_custom">
				<anchored to="dicecustom" position="righthigh" offset="5,1" width="10" />
				<static>+</static>
			</label>
      
			<cycler_power name="stat">
				<anchored to="label_plus_custom" position="righthigh" offset="2,1" width="30" height="20" />
			</cycler_power>
			<label name="label_plus2">
				<anchored to="stat" position="righthigh" offset="5,0" width="10" />
				<static>+</static>
			</label>
			<basicnumber name="bonus">
				<anchored to="label_plus2" position="righthigh" offset="2,1" width="35" height="20" />
				<hideonvalue>0</hideonvalue>
			</basicnumber>
			<stringu name="type">
				<anchored to="bonus" position="righthigh" offset="10,0" height="20">
					<right parent="rightanchor" anchor="left" relation="relative" offset="-5" />
				</anchored>
			</stringu>
      
			<label name="label_x">
				<anchored to="label_dee" position="belowleft" width="10" offset="0,3" />
				<static>X</static>
			</label>
      
			<basicnumber name="customvalue">
				<anchored to="dice" position="belowleft" width="60" height="20" offset="0,3" />
        <hideonvalue>0</hideonvalue>
        <tooltip textres="power_label_customvalue_tooltip" />
			</basicnumber>
      
		</sheetdata>
